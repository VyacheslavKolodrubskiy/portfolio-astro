---
import BaseHead from './BaseHead.astro'
import type { HeadProps } from '@shared/types.common.ts'
import '@styles/global.css'

const { title, description } = Astro.props as HeadProps
---

<!doctype html>
<html lang="en">
  <BaseHead
    {title}
    {description}
  />
  <body>
    <slot />
    <script src="@libs/lenis.ts"></script>
  </body>
</html>
